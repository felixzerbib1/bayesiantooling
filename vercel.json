{
  "buildCommand": "node generate.js",
  "outputDirectory": "public",
  "framework": null,
  "installCommand": "echo 'No dependencies to install'",
  "routes": [
    { "src": "/feature-flags.md", "headers": [{ "key": "Content-Type", "value": "text/markdown; charset=utf-8" }] },
    { "src": "/templates/(.*)\\.csv", "headers": [{ "key": "Content-Disposition", "value": "attachment" }] }
  ]
}
